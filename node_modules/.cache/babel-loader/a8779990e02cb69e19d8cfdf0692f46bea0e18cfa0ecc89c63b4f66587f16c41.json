{"ast":null,"code":"// eslint-disable-next-line\nimport Header from \"./components/Header.vue\";\nimport Footer from \"./components/Footer.vue\";\nimport { store } from './components/store.js';\nimport { onMounted } from \"vue\";\nconst getCookie = name => {\n  return document.cookie.split(\"; \").reduce((r, v) => {\n    const parts = v.split(\"=\");\n    return parts[0] === name ? decodeURIComponent(parts[1]) : r;\n  }, \"\");\n};\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Footer\n  },\n  data() {\n    return {\n      store\n    };\n  },\n  beforeMount() {\n    // check for a cookie\n    let data = getCookie(\"_site_data\");\n    if (data !== \"\") {\n      let cookieData = JSON.parse(data);\n\n      // update store\n      store.token = cookieData.token.token;\n      store.user = {\n        id: cookieData.user.id,\n        first_name: cookieData.user.first_name,\n        last_name: cookieData.user.last_name,\n        email: cookieData.user.email\n      };\n    }\n  },\n  mounted() {\n    const payload = {\n      foo: \"bar\"\n    };\n\n    // const headers = new Headers();\n    // headers.append(\"Content-Type\", \"application/json\");\n    // headers.append(\"Authorization\", \"Bearer \" + store.token);\n\n    const requestOptions = {\n      method: \"POST\",\n      body: JSON.stringify(payload)\n      // headers: headers,\n    };\n    fetch(\"http://localhost:8081/admin/foo\", requestOptions).then(response => response.json()).then(data => {\n      console.log(data);\n    });\n  }\n};","map":{"version":3,"names":["Header","Footer","store","onMounted","getCookie","name","document","cookie","split","reduce","r","v","parts","decodeURIComponent","components","data","beforeMount","cookieData","JSON","parse","token","user","id","first_name","last_name","email","mounted","payload","foo","requestOptions","method","body","stringify","fetch","then","response","json","console","log"],"sources":["/Users/ed1p_/MovieProject/frontend/src/App.vue"],"sourcesContent":["<template>\n  <Header />\n  <div>\n    <router-view/>\n  </div>\n  <Footer />\n</template>\n\n<script>\n// eslint-disable-next-line\nimport Header from \"./components/Header.vue\"\nimport Footer from \"./components/Footer.vue\"\nimport {store} from './components/store.js'\nimport {onMounted} from \"vue\";\nconst getCookie = (name) => {\n  return document.cookie.split(\"; \").reduce((r, v) => {\n    const parts = v.split(\"=\");\n    return parts[0] === name ? decodeURIComponent(parts[1]) : r;\n  }, \"\");\n}\n\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Footer,\n  },\n  data() {\n    return {\n      store\n    }\n  },\n  beforeMount() {\n    // check for a cookie\n    let data = getCookie(\"_site_data\");\n\n    if (data !== \"\") {\n      let cookieData = JSON.parse(data);\n\n      // update store\n      store.token = cookieData.token.token;\n      store.user = {\n        id: cookieData.user.id,\n        first_name: cookieData.user.first_name,\n        last_name: cookieData.user.last_name,\n        email: cookieData.user.email,\n      }\n    }\n  },\n  mounted() {\n    const payload = {\n      foo: \"bar\",\n    }\n\n    // const headers = new Headers();\n    // headers.append(\"Content-Type\", \"application/json\");\n    // headers.append(\"Authorization\", \"Bearer \" + store.token);\n\n    const requestOptions = {\n      method: \"POST\",\n      body: JSON.stringify(payload),\n      // headers: headers,\n    }\n\n    fetch(\"http://localhost:8081/admin/foo\", requestOptions)\n        .then((response) => response.json())\n        .then((data) => {\n          console.log(data);\n        })\n  }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AASA;AACA,OAAOA,MAAK,MAAO,yBAAwB;AAC3C,OAAOC,MAAK,MAAO,yBAAwB;AAC3C,SAAQC,KAAK,QAAO,uBAAsB;AAC1C,SAAQC,SAAS,QAAO,KAAK;AAC7B,MAAMC,SAAQ,GAAKC,IAAI,IAAK;EAC1B,OAAOC,QAAQ,CAACC,MAAM,CAACC,KAAK,CAAC,IAAI,CAAC,CAACC,MAAM,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;IAClD,MAAMC,KAAI,GAAID,CAAC,CAACH,KAAK